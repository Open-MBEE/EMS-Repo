<webscript> 
  <shortname>Delete elements</shortname> 
  <description>
  Deletes an element recursively - each delete in its own transaction
  </description>
  <url>/javawebscripts/elements/{id}</url>
  <format default="json">any</format> 
  <authentication>user</authentication> 
  <transaction>none</transaction>
  <family>EMS Java Webscripts</family>
</webscript>