<webscript>
  <shortname>Toggle simpleCache on or off</shortname>
  <description>toggle simpleCache off</description>
  <url>/simpleCache?on={on?}&amp;off={off?}</url>
  <authentication>user</authentication>
  <format default="html">argument</format>
  <family>EMS Utils</family>
  <args>
    <arg>
        <name>on</name>
        <description>Optional, if specified will turn doSimpleCaching on</description>
    </arg>
    <arg>
        <name>off</name>
        <description>Optional, if specified will turn doSimpleCaching off</description>
    </arg>
  </args>
</webscript>
