<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="view:viewEditorModel">
    <description>Extension model</description>
    <author>JPL</author>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
    </imports>
    <namespaces>
        <namespace uri="http://jpl.nasa.gov/model/view/1.0" prefix="view"/>
    </namespaces>
    <data-types/>
    <constraints/>
    <types>
        <type name="view:Source">
            <parent>cm:content</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="view:Text">
            <parent>view:Source</parent>
            <properties>
                <property name="view:text">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="view:ModelElement">
            <parent>view:Source</parent>
            <properties>
                <property name="view:documentation">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
                <property name="view:mdid">
                    <type>d:text</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="false"/>
                </property>
                <property name="view:name">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                    	<atomic>false</atomic>
                    	<stored>false</stored>
                    	<tokenised>true</tokenised>
                    </index>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects>
            	<aspect>cm:versionable</aspect>
           <!--   	<aspect>cm:lockable</aspect> -->
            </mandatory-aspects>
        </type>
        <type name="view:Comment">
            <parent>view:ModelElement</parent>
            <properties>
                <property name="view:committed">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
                <property name="view:deleted">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
            </properties>
  <!--      <associations> replies not used right now (for comment threading)
                <association name="view:replies">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>view:Comment</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations> -->
            <overrides/>
            <mandatory-aspects/>
         </type>
        <type name="view:Volume">
            <parent>view:ModelElement</parent>
            <properties>
            	<property name="view:rootVolume">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
            </properties>
            <associations>
            	<association name="view:volumes">
            		<source>
            			<mandatory>false</mandatory>
            			<many>false</many>
            		</source>
            		<target>
            			<class>view:Volume</class>
            			<mandatory>false</mandatory>
            			<many>true</many>
            		</target>
            	</association>
            	<association name="view:documents">
            		<source>
            			<mandatory>false</mandatory>
            			<many>false</many>
            		</source>
            		<target>
            			<class>view:DocumentView</class>
            			<mandatory>false</mandatory>
            			<many>true</many>
            		</target>
            	</association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="view:View">
            <parent>view:ModelElement</parent>
            <properties>
                <property name="view:noSection">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
                <property name="view:viewsJson">
                	<type>d:text</type>
                	<mandatory>false</mandatory>
                	<index enabled="false"/>
                </property>
                <property name="view:containsJson">
                	<type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
                <property name="view:sourcesJson">
                	<type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
            </properties>
            <associations>
                <association name="view:views">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>view:View</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
                <association name="view:comments">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>view:Comment</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
                <association name="view:contains">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>view:PresentationElement</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects>
                <aspect>cm:taggable</aspect>
                <aspect>view:Orderable</aspect>
            </mandatory-aspects>
        </type>
         <type name="view:DocumentView">
            <parent>view:View</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="view:Property">
            <parent>view:ModelElement</parent>
            <properties>
                <property name="view:defaultValue">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="view:PresentationElement">
            <parent>cm:content</parent>
            <properties>
                <property name="view:useProperty">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
            </properties>
            <associations>
                <association name="view:source">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>view:Source</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects>
                <aspect>view:Orderable</aspect>
            </mandatory-aspects>
        </type>
        <type name="view:Table">
            <parent>view:PresentationElement</parent>
            <properties>
                <property name="view:body">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
                <property name="view:header">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
                <property name="view:style">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>                
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects>
                <aspect>view:Captioned</aspect>
                <aspect>view:Titled</aspect>
            </mandatory-aspects>
        </type> 
        <type name="view:Paragraph">
            <parent>view:PresentationElement</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>        
    </types>
    <aspects>
        <aspect name="view:Orderable">
            <properties>
                <property name="view:index">
                    <type>d:int</type>
                    <mandatory>false</mandatory>
                    <index enabled="false"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="view:Captioned">
            <properties>
                <property name="view:caption">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="view:Titled">
            <properties>
                <property name="view:title">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
    </aspects>
</model>
