<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<bean id="webscript.gov.nasa.jpl.javawebscripts.view.post"
		class="gov.nasa.jpl.view_repo.webscripts.ViewPostJson"
		parent="webscript">
        <property name="repositoryHelper" ref="repositoryHelper" />
        <property name="services" ref="ServiceRegistry" />
        <property name="jwsUtil" ref="jwsUtil" />
	</bean>
	
	<bean id="webscript.gov.nasa.jpl.javawebscripts.project.post"
		class="gov.nasa.jpl.view_repo.webscripts.ProjectPost"
		parent="webscript">
        <property name="repositoryHelper" ref="repositoryHelper" />
        <property name="services" ref="ServiceRegistry" />
        <property name="jwsUtil" ref="jwsUtil" />
	</bean>
</beans>