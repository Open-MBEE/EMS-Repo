<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="ems:emsModel">
    <description>EMS Unique Models</description>
    <author>JPL</author>
    <version>1.0</version>
    
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <import uri="http://jpl.nasa.gov/model/view/2.0" prefix="view2"/>
    </imports>
    
    <namespaces>
        <namespace uri="http://jpl.nasa.gov/model/ems/1.0" prefix="ems"/>
    </namespaces>
    
    <data-types/>
    
    <types>      
        <type name="ems:Job">
            <parent>cm:content</parent>
            <properties>
	            	<property name="ems:job_status">
	                	<type>d:text</type>
	                	<mandatory>true</mandatory>
	                	<index enabled="false"/>
				    <constraints>
				            <constraint name="ems:job_status_constraint" type="LIST">
				                <parameter name="allowedValues">
				                    <list>
				                        <value>Active</value>
				                        <value>Succeeded</value>
				                        <value>Failed</value>
				                    </list>
				                </parameter>
				            </constraint>
				    </constraints>
                </property>
                <property name="ems:job_log">
	                	<type>d:noderef</type>
	                	<mandatory>false</mandatory>
	                	<multiple>false</multiple>
	                	<index enabled="false"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects>
	            	<aspect>cm:indexControl</aspect>
	            	<aspect>cm:versionable</aspect>
	            	<aspect>cm:taggable</aspect>
	            	<aspect>cm:titled</aspect>
            </mandatory-aspects>
        </type>
        
        <type name="ems:ConfigurationSet">
            <parent>ems:Job</parent>
            <properties/>
            <associations>
            		<association name="ems:configuredSnapshots">
            			<source>
            				<mandatory>false</mandatory>
            				<many>true</many>
            			</source>
            			<target>
            				<class>view2:Snapshot</class>
            				<mandatory>false</mandatory>
            				<many>true</many>
            			</target>
            		</association>
            		<association name="ems:configuredProducts">
            		  <source>
            		      <mandatory>false</mandatory>
            		      <many>true</many>
            		  </source>
            		  <target>
            		      <class>view2:Product</class>
            		      <mandatory>false</mandatory>
            		      <many>true</many>
            		  </target>
            		</association>
            </associations>
            <overrides/>
            <mandatory-aspects>
                <aspect>view2:timestamped</aspect>
            </mandatory-aspects>
        </type>
     </types>
     
     <aspects>
		<aspect name="ems:HasChildren">
		    <properties>
		        <property name="ems:children">
		            <type>d:noderef</type>
		            <mandatory>false</mandatory>
		            <multiple>true</multiple>
		            <index enabled="true">
		                <atomic>false</atomic>
		                <stored>false</stored>
		                <tokenised>false</tokenised>
		            </index>
		        </property>
		    </properties>
		</aspect>
        
         <aspect name="ems:TreeNode">
            <parent>ems:HasChildren</parent>
            <properties>
                <property name="ems:parent">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
            </properties>
        </aspect>
        
        <aspect name="ems:Workspace">
            <parent>ems:TreeNode</parent>
            <properties>
			<!-- workspace_name is used instead of cm_name because we put all workspaces 
				in the users' home folders, which require unique cm:names, and there can 
				be two workspaces with the same name but different parent workspaces -->
                <property name="ems:workspace_name">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
				<!-- This is the last time the changes from the parent workspace were applied 
					to this one. -->
                <property name="ems:lastTimeSyncParent">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false"/>
                </property>
				<!-- The time point at which the workspace is a copy. If a workspace does 
					not have this property, the workspace "walks" with its parent; in other words,
					changes in the parent workspace pass through to the child (unless they conflict). -->
                <property name="ems:copyTime">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false"/>
                </property>
                <property name="ems:description">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false"/>
                </property>
                <property name="ems:permission">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false"/>
                </property>
            </properties>
        </aspect>
        
        <aspect name="ems:HasWorkspace">
            <properties>
                <property name="ems:workspace">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
                <property name="ems:source"> <!-- The node from the parent workspace from which this one was cloned -->
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
            </properties>
        </aspect>

        <!-- Trashed is to implement a trash can in case we need something that 
             we would lose if deleting from Alfresco -->
        <aspect name="ems:Trashed"/>
        
        <aspect name="ems:Added"/>
        <aspect name="ems:Deleted"/>
        <aspect name="ems:Moved"/>
        <aspect name="ems:Updated"/>

        <!-- The merge source is a node that overwrote this one when merged from 
        a child workspace. -->
        <aspect name="ems:MergeSource">
            <properties>
                <property name="ems:mergeSource">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false"/>
                </property>
            </properties>
        </aspect>
        
        <aspect name="ems:Owned">
          <properties>
              <property name="ems:owner">
                  <type>d:noderef</type>
                  <mandatory>false</mandatory>
                  <multiple>false</multiple>
                  <index enabled="false">
                      <atomic>false</atomic>
                      <stored>false</stored>
                      <tokenised>false</tokenised>
                  </index>
              </property>
              <property name="ems:ownedChildren">
                  <type>d:noderef</type>
                  <mandatory>false</mandatory>
                  <multiple>true</multiple>
                  <index enabled="false">
                      <atomic>false</atomic>
                      <stored>false</stored>
                      <tokenised>false</tokenised>
                  </index>
              </property>
          </properties>
        </aspect>
        
        <aspect name="ems:Reified">
          <properties>
              <property name="ems:reifiedNode">
                  <type>d:noderef</type>
                  <mandatory>false</mandatory>
                  <multiple>false</multiple>
                  <index enabled="false">
                      <atomic>false</atomic>
                      <stored>false</stored>
                      <tokenised>false</tokenised>
                  </index>
              </property>
              <property name="ems:reifiedPkg">
                  <type>d:noderef</type>
                  <mandatory>false</mandatory>
                  <multiple>false</multiple>
                  <index enabled="false">
                      <atomic>false</atomic>
                      <stored>false</stored>
                      <tokenised>false</tokenised>
                  </index>
              </property>
          </properties>
        </aspect>
        
        <aspect name="ems:Committable">
            <properties>
                <property name="ems:commitParents">
                    <!-- multiple parents only for MERGE -->
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="false"/>
                </property>
                <property name="ems:commitChildren">
                    <!-- multiple children only for BRANCH -->
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="false"/>
                </property>
                <property name="ems:commitType">
                    <!-- can be COMMIT, MERGE, BRANCH -->
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false"/>
				    <constraints>
				        <constraint name="ems:commitType_constraint" type="LIST">
				            <parameter name="allowedValues">
				                <list>
				                    <value>COMMIT</value>
				                    <value>MERGE</value>
				                    <value>BRANCH</value>
				                </list>
				            </parameter>
				        </constraint>
				    </constraints>
                </property>
                <property name="ems:commit">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false"/>
                </property>
            </properties>
        </aspect>        
        <aspect name="ems:MagicDrawData">
          <properties>
              <property name="ems:mdData">
                  <type>d:text</type>
                  <mandatory>false</mandatory>
                  <multiple>false</multiple>
                  <index enabled="false">
                      <atomic>false</atomic>
                      <stored>false</stored>
                      <tokenised>false</tokenised>
                  </index>
              </property>
          </properties>
        </aspect>
        <aspect name="ems:BranchPerm">
          <properties>
              <property name="ems:ldapGroup">
                  <type>d:text</type>
                  <mandatory>false</mandatory>
                  <multiple>false</multiple>
                  <index enabled="false">
                      <atomic>false</atomic>
                      <stored>false</stored>
                      <tokenised>false</tokenised>
                  </index>
              </property>
          </properties>
        </aspect>
        <aspect name="ems:Site">
            <properties>
                 <property name="ems:siteParent">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
                <property name="ems:siteChildren">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="true">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
                <property name="ems:sitePackage">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
            </properties>
        </aspect>
        <aspect name="ems:SiteCharacterization">
            <properties>
                <property name="ems:siteSite">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="false">
                        <atomic>false</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
            </properties>
        </aspect>
     </aspects>

</model>
